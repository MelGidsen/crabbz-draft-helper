{
  "main": "dist/main.js",
  "name": "crabbz-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "electron .",
    "build": "ng build --base-href ./ && tsc --lib ES2018,DOM --target ES5 src-be/main.ts --outDir dist && tsc --lib ES2018,DOM --target ES5 src-be/preload.ts --outDir dist && tsc --lib ES2018,DOM --target ES5 src-be/renderer.ts --outDir dist",
    "test": "ng test",
    "lint": "eslint -c .eslintrc --ext .ts ./src"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
